{
  "name": "arnlinvoices/invoicemanager",
  "description": "A reusable Laravel Invoice Manager module",
  "homepage": "https://github.com/hrik003/invoicemanager",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "lumen",
    "package",
    "development",
    "starter",
    "invoicemanager",
    "management",
    "invoice"
  ],
  "authors": [
    {
      "name": "Arijit Samaddar",
      "email": "samaddar_arijit@rediffmail.com",
      "role": "Developer"
    }
  ],
  "require": {
      "php": "^8.1",
      "barryvdh/laravel-dompdf": "^3.0",
      "simplesoftwareio/simple-qrcode": "^4.2",
      "maatwebsite/excel": "^3.1"
  },
  "require-dev": {
      "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "ArnlInvoices\\InvoiceManager\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "ArnlInvoices\\InvoiceManager\\InvoiceManagerServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
